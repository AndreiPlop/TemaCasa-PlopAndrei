<body style="background-color:rgb(139, 134, 134);">
    <style>
           .title {
          font-family: "Lucida Console", "Courier New", monospace;
          font-size: 30px;
          color: rgb(28, 79, 138);
          text-shadow: 4px 4px 6px #000000;
      }
      .button {
  background-color: #112846;
  border: none;
  color: white;
  padding: 20px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 2px;
  cursor: pointer;
}
.button2 {border-radius:50%;}
    </style>
        <h1 class="title">Pariuri Tenis</h1>
    </br>
        <form action="/tenis2" method="post">
            <% for(let i=0;i<intrebari.length;i++) { %>
            <p><%- intrebari[i].intrebare %></p>
            <% for(let j=0;j<intrebari[i].variante.length;j++) { %>
            <input type="radio" name="q<%- i %>" value="<%- j %>" /><%- intrebari[i].variante[j] %><br />
            <% } %>
            <br>
            <% } %>
    
            <input class="button button2" type="submit" value="Urmatorul" />
        </form>
    </body>